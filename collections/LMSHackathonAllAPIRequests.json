			"item": [
				{
					"name": "Positive Tests",
					"item": [
						{
							"name": "Login - Valid Credential",
							"event": [
								{
									"listen": "test",
									"script": {
										"exec": [
											"",
											"// Status code verification",
											"pm.test(\"Status code is successful\", function () {",
											"    pm.expect(pm.response.code).to.be.oneOf([200, 201, 204]);",
											"});",
											"",
											"// Response time check",
											"pm.test(\"Response time is less than 2000ms\", function () {",
											"    pm.expect(pm.response.responseTime).to.be.below(2000);",
											"});",
											"",
											"// Content-Type verification",
											"pm.test(\"Content-Type is application/json\", function () {",
											"    pm.response.to.have.header(\"Content-Type\");",
											"    pm.expect(pm.response.headers.get(\"Content-Type\")).to.include(\"application/json\");",
											"});",
											"",
											"// Verify response structure",
											"pm.test(\"Response has required fields\", function () {",
											"    var jsonData = pm.response.json();",
											"    pm.expect(jsonData).to.have.property('token');",
											"    pm.expect(jsonData).to.have.property('userId');",
											"    pm.expect(jsonData).to.have.property('email');",
											"    pm.expect(jsonData).to.have.property('roles');",
											"    pm.expect(jsonData).to.have.property('status');",
											"});",
											"",
											"// Verify token is not null",
											"pm.test(\"JWT token is present\", function () {",
											"    var jsonData = pm.response.json();",
											"    pm.expect(jsonData.token).to.not.be.null;",
											"    pm.expect(jsonData.token).to.be.a('string');",
											"});",
											"",
											"// Verify roles array",
											"pm.test(\"Roles array is present\", function () {",
											"    var jsonData = pm.response.json();",
											"    pm.expect(jsonData.roles).to.be.an('array');",
											"    pm.expect(jsonData.roles.length).to.be.above(0);",
											"});",
											"",
											"// Save token",
											"",
											"// Save JWT token to environment",
											"if (pm.response.code === 200) {",
											"    var jsonData = pm.response.json();",
											"    if (jsonData.token) {",
											"        pm.environment.set(\"authToken\", jsonData.token);",
											"        pm.collectionVariables.set(\"authToken\", jsonData.token);",
											"        console.log(\"Token saved: \" + jsonData.token);",
											"    }",
											"}",
											""
										],
										"type": "text/javascript",
										"packages": {},
										"requests": {}
									}
								}
							],
							"request": {
								"method": "POST",
								"header": [
									{
										"key": "Content-Type",
										"value": "application/json",
										"type": "text"
									}
								],
								"body": {
									"mode": "raw",
									"raw": "{\n  \"userLoginEmailId\": \"team1@gmail.com\",\n  \"password\": \"ApiHackathon@2025\"\n}",
									"options": {
										"raw": {
											"language": "json"
										}
									}
								},
								"url": {
									"raw": "{{baseUrl}}/login",
									"host": [
										"{{baseUrl}}"
									],
									"path": [
										"login"
