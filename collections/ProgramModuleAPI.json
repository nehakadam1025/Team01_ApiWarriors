				{
					"name": "Get All Programs",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test(\"Response is an array\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.be.an('array');",
									"});",
									"",
									"var programs = pm.response.json();",
									"pm.test(\"Each program contains required fields\", function () {",
									"    ",
									"    programs.forEach(function(program) {",
									"        pm.expect(program).to.have.property('programId');",
									"        pm.expect(program).to.have.property('programName');",
									"        pm.expect(program).to.have.property('programDescription');",
									"        pm.expect(program).to.have.property('programStatus');",
									"        pm.expect(program).to.have.property('creationTime');",
									"        pm.expect(program).to.have.property('lastModTime');",
									"    });",
									"});",
									"",
									"function randomLetters(len) {",
									"    const letters = \"ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz\";",
									"    let s = \"\";",
									"    for (let i = 0; i < len; i++) {",
									"        s += letters.charAt(Math.floor(Math.random() * letters.length));",
									"    }",
									"    return s;",
									"}",
									"",
									"if (programs && programs.length > 0) {",
									"    const activePrograms = programs.filter(program => ",
									"        program.programStatus && program.programStatus.toLowerCase() === 'active'",
									"    );",
									"",
									"    if (activePrograms && activePrograms.length > 0) {",
									"        const sorted = activePrograms.sort((a, b) => a.programId - b.programId);",
									"        ",
									"        // For GET operations",
									"        pm.environment.set(\"getProgramId\", sorted[0].programId);",
									"        ",
									"        // For CREATE - generate unique names",
									"        //pm.environment.set(\"newProgramName\", `TestProg-${timestamp}`);",
									"        // generate a random name (e.g. 10 letters) and optionally add a hyphen",
									"        const randomName = randomLetters(4);",
									"        pm.environment.set(\"newProgramName\", `TestProg-${randomName}`); ",
									"    ",
									"        const timestamp = Date.now();",
									"",
									"        // For UPDATE operations",
									"        if (sorted.length > 1) {",
									"            pm.environment.set(\"updateByProgramId\", sorted[1].programId);",
									"            pm.environment.set(\"updateByProgramName\", sorted[1].programName);",
									"",
									"            pm.environment.set(\"updatedProgramDescription\", `Updated-${timestamp}`); ",
									"        }",
									"        ",
									"        // For DELETE operations",
									"        if (sorted.length > 2) {",
									"            pm.environment.set(\"deleteProgramId\", sorted[2].programId);",
									"        }",
									"        console.log(\"Test data setup complete!\");",
									"    }",
									"}"
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
						}
					],
