									"        let user = response[0];",
									"",
									"        // Mandatory fields",
									"        pm.expect(user).to.have.property(\"userId\").that.is.a(\"string\");",
									"        pm.expect(user).to.have.property(\"userFirstName\").that.is.a(\"string\");",
									"        pm.expect(user).to.have.property(\"userLastName\").that.is.a(\"string\");",
									"        pm.expect(user).to.have.property(\"userLocation\").that.is.a(\"string\");",
									"        pm.expect(user).to.have.property(\"userTimeZone\").that.is.a(\"string\");",
									"        pm.expect(user).to.have.property(\"userLinkedinUrl\").that.is.a(\"string\");",
									"        pm.expect(user).to.have.property(\"userVisaStatus\").that.is.a(\"string\");",
									"        pm.expect(user).to.have.property(\"userEduUg\").that.is.a(\"string\");",
									"        pm.expect(user).to.have.property(\"userEduPg\").that.is.a(\"string\");",
									"        pm.expect(user).to.have.property(\"userComments\");",
									"",
									"        //  Optional fields (string or null allowed)",
									"        if (user.hasOwnProperty(\"userMiddleName\")) {",
									"            pm.expect(",
									"                user.userMiddleName === null || typeof user.userMiddleName === \"string\",",
									"                \"userMiddleName should be string or null\"",
									"            ).to.be.true;",
									"        }",
									"",
									"        if (user.hasOwnProperty(\"userLoginEmail\")) {",
									"            pm.expect(",
									"                user.userLoginEmail === null || typeof user.userLoginEmail === \"string\",",
									"                \"userLoginEmail should be string or null\"",
									"            ).to.be.true;",
									"        }",
									"",
									"    } else {",
									"        console.warn(\"Response array is empty, skipping field validation.\");",
									"    }",
									"});",
									"",
									"// Validate Email format if present",
									"pm.test(\"Validate email format (if provided)\", function () {",
									"    let user = pm.response.json()[0];",
									"    if (user && user.userLoginEmail) {",
									"        pm.expect(user.userLoginEmail).to.match(/^[\\w-.]+@([\\w-]+\\.)+[\\w-]{2,4}$/);",
									"    } else {",
									"        console.warn(\"Email is null or not provided, skipping validation.\");",
									"    }",
									"});",
									"",
									"// Validate LinkedIn URL format (if provided)",
									"pm.test(\"Validate LinkedIn URL format (if provided)\", function () {",
									"    let user = pm.response.json()[0];",
									"    if (user && user.userLinkedinUrl) {",
									"        pm.expect(user.userLinkedinUrl).to.match(/^https?:\\/\\/(www\\.)?linkedin\\.com\\/.+/);",
									"    } else {",
									"        console.warn(\"LinkedIn URL is null or not provided, skipping validation.\");",
									"    }",
									"});",
									"",
									"//  Validate phone number (if provided)",
									"pm.test(\"Validate phone number (if provided)\", function () {",
									"    let user = pm.response.json()[0];",
									"    if (user && user.userPhoneNumber) {",
									"        pm.expect(user.userPhoneNumber.toString().length).to.be.at.least(10);",
									"    } else {",
									"        console.warn(\"Phone number not provided, skipping validation.\");",
									"    }",
